{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "allowUmdGlobalAccess": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "pages/**/*",
    "src/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
